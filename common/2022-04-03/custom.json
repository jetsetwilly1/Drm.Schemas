{
  "id": "https://schemas.drmtemplates.io/2022-04-03/custom.json#",
  "title": "Entity definition for CustomEntityProcessing",
  "definitions": {
    "CustomEntityProcessingPropertiesCreateParameters": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "entityname": {
          "description": "The collection entity name",
          "type": "string"
        },
        "data": {
          "description": "Must be of type 'data to patch'",
          "type": "array",
          "minItems": 1
        },
        "drmmetadata": {
          "description": "Metadata optional object used to describe this entry.",
          "type": "object"
        }
      },
      "required": [
        "entityname"
      ]
    }
  }
}